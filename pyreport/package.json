{
  "name": "pyreport",
  "displayName": "PyReport##",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "icon.jpg",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pyreport.getVariables",
        "title": "Generate PyReport",
        "icon": "$(preview)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "pyreport.getVariables",
          "when": "resourceExtname == .ipynb",
          "group": "navigation"
        }
      ]
    }
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
